        <class_name>RowProcessor</class_name>

        <class_source><![CDATA[
import org.apache.activemq.ActiveMQConnection;
import org.apache.activemq.ActiveMQConnectionFactory;

import javax.jms.*;

private static String url = ActiveMQConnection.DEFAULT_BROKER_URL;
//private static String url = "http://142.103.25.29:8080";
private static Session session;
private static MessageConsumer consumer;
private static Connection connection;

int outputRowSize;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws JMSException, KettleException 
{
	
	 Message message = consumer.receive();
	 MapMessage mapMessage = (MapMessage) message;

	 Object[] r = new Object[data.outputRowMeta.size()];

	 if (r == null) { 
	 setOutputDone();
	 return false;
	 }

	 if (first) {
	 first = false;
	 outputRowSize = data.outputRowMeta.size();
 	 }

	 r = createOutputRow(r, outputRowSize);
	 get(Fields.Out, "band0").setValue(r, mapMessage.getDouble("band0"));
	 get(Fields.Out, "band1").setValue(r, mapMessage.getDouble("band1"));
	 get(Fields.Out, "band2").setValue(r, mapMessage.getDouble("band2"));
	 get(Fields.Out, "band3").setValue(r, mapMessage.getDouble("band3"));
	 get(Fields.Out, "band4").setValue(r, mapMessage.getDouble("band4"));
	 get(Fields.Out, "band5").setValue(r, mapMessage.getDouble("band5"));
	 get(Fields.Out, "band6").setValue(r, mapMessage.getDouble("band6"));
	 get(Fields.Out, "band7").setValue(r, mapMessage.getDouble("band7"));
	 get(Fields.Out, "band8").setValue(r, mapMessage.getDouble("band8"));
	 get(Fields.Out, "band9").setValue(r, mapMessage.getDouble("band9"));


	 putRow(data.outputRowMeta, r);

	 setOutputDone();


  // Never end, keep running indefinitely, always return true
  //

  return false;
	


}

public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface) {
   if (super.init(stepMetaInterface, stepDataInterface)) {

try {

     ConnectionFactory connFactory = new ActiveMQConnectionFactory(url);

	 connection = connFactory.createConnection(getParameter("USER"), getParameter("PASS"));

 	 connection.start();

	 session = connection.createSession(false, Session.AUTO_ACKNOWLEDGE);

	 Topic destination = session.createTopic(getParameter("QNAME"));
	 
	 consumer = session.createConsumer(destination);

     return true;


} catch (JMSException e) {
logError("Unable to close JMS objects", e);
}

   }
   return false;
}

public void dispose(StepMetaInterface smi, StepDataInterface sdi) {

try {

consumer.close();
session.close();
connection.stop();
connection.close();

} catch (JMSException e) {
logError("Unable to close JMS objects", e);
}
super.dispose(smi, sdi);
}




]]></class_source>
